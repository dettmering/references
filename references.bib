	Journal = {J Cell Physiol},
	Journal-Full = {Journal of cellular physiology},
	Mesh = {Cell Aging; Cell Line; Chromosome Duplication; Chromosomes, Human; Cytogenetic Analysis; Fibroblasts; Humans; In Situ Hybridization, Fluorescence; Telomere},
	Month = {May},
	Number = {5},
	Pages = {1932-40},
	Pmid = {21732364},
	Pst = {ppublish},
	Timestamp = {2014.05.10},
	Title = {Duplicated chromosomal fragments stabilize shortened telomeres in normal human IMR-90 cells before transition to senescence},
